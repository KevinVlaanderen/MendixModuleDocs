<h3>Domain model</h3>
<p>{{Documentation}}</p>

<h4>Entities</h4>
{{#HasEntities}}

<table>
    <colgroup>
        <col style="width: 20%"/>
        <col style="width: 20%"/>
        <col style="width: 60%"/>
    </colgroup>
    <thead>
        <tr>
            <th>Name</th>
            <th>Generalization</th>
            <th>Documentation</th>
        </tr>
    </thead>
    <tbody>
        {{#Entities}}
        <tr>
            <td><{{Name}}</td>
            <td>{{Generalization}}</td>
            <td>{{Documentation}}</td>
        </tr>
        {{/Entities}}
    </tbody>
</table>
{{/HasEntities}}
{{^HasEntities}}
