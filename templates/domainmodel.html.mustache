<h5>Associations</h5>
{{#HasAssociations}}
<table>
    <colgroup>
        <col style="width: 20%"/>
        <col style="width: 20%"/>
        <col style="width: 20%"/>
        <col style="width: 40%"/>
    </colgroup>
    <thead>
        <tr>
            <th>Name</th>
            <th>Connected to</th>
            <th>Multiplicity</th>
            <th>Documentation</th>
        </tr>
    </thead>
    <tbody>
        {{#Associations}}
        <tr>
            <td>{{Name}}</td>
            <td>{{OtherSide}}</td>
            <td>{{Multiplicity}}</td>
            <td>{{Documentation}}</td>
        </tr>
