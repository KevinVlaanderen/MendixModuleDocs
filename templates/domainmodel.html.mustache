<p>{{Documentation}}</p>

{{#Generalization}}
<h5>Generalization</h5>
<p>{{Generalization}}</p>
{{/Generalization}}

<h5>Attributes</h5>
{{#HasAttributes}}
<table>
    <colgroup>
        <col style="width: 20%"/>
        <col style="width: 20%"/>
        <col style="width: 20%"/>
        <col style="width: 40%"/>
    </colgroup>
    <thead>
        <tr>
            <th>Name</th>
            <th>Type</th>
            <th>Default value</th>
            <th>Documentation</th>
        </tr>
    </thead>
    <tbody>
        {{#Attributes}}
        <tr>
            <td>{{Name}}</td>
            <td>{{Type}}</td>
            <td>{{DefaultValue}}</td>
            <td>{{Documentation}}</td>
        </tr>
        {{/Attributes}}
    </tbody>
</table>
{{/HasAttributes}}
